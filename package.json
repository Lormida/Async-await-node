{
  "name": "asynchronous_travel_around_the_node.js",
  "version": "1.0.0",
  "description": "Event loop tasks, event loop debugging, baseline performance(libuv), worker threads, workepools, multiprocessing(clustering), semaphores, mutexes, atomics",
  "license": "MIT",
  "keywords": [
    "multithreading",
    "multiprocessing",
    "concurrency",
    "promises",
    "event-loop",
    "node.js",
    "performance-node",
    "async-debug",
    "threads",
    "libuv",
    "workers",
    "workerpool",
    "parallelism",
    "clustering",
    "semaphore",
    "mutex",
    "atomics"
  ],
  "type": "module",
  "types": "global.d.ts",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lormida/Asynchronous_travel_around_the_Node.js.git"
  },
  "scripts": {
    "dev": "nodemon -e ts --watch src .env --exec \"tsc -p tsconfig.json --incremental && node --experimental-specifier-resolution=node --no-warnings ./build/src/main.js\"",
    "dev:performance": "nodemon -e ts --watch src .env --exec \"tsc -p tsconfig.json --incremental && UV_THREADPOOL_SIZE=12 node --experimental-specifier-resolution=node --no-warnings ./build/src/main.js\"",
    "start": "node --experimental-specifier-resolution=node --no-warnings ./build/src/main.js",
    "start:performance": "UV_THREADPOOL_SIZE=12 node --experimental-specifier-resolution=node --no-warnings ./build/src/main.js",
    "start:debug": "node --experimental-specifier-resolution=node --inspect-brk ./build/src/main.js",
    "ts:check": "tsc --noEmit",
    "copyMySpecialFiles": "copyfiles --error --up 1 src/**/*.{jpg,mp4} build/src",
    "prebuild": "rm -rf build && npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "postbuild": "npm run copyMySpecialFiles",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts --fix",
    "format": "prettier --config .prettierrc.cjs --write .",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/jest": "~29.5",
    "@types/node": "~20",
    "@typescript-eslint/eslint-plugin": "~6.7.3",
    "@typescript-eslint/parser": "~6.7.3",
    "copyfiles": "^2.4.1",
    "eslint": "~8.50",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-jest": "~27.4",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "~29.7",
    "nodemon": "^3.0.1",
    "prettier": "~3.0",
    "rimraf": "~5.0",
    "ts-jest": "~29.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2"
  },
  "dependencies": {
    "@types/workerpool": "^6.4.4",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.3",
    "morgan": "^1.10.0",
    "node-worker-threads-pool": "^1.5.1",
    "sharp": "^0.32.6",
    "tslib": "~2.6",
    "workerpool": "^6.5.0"
  }
}
